<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:id="@+id/test_result"
	android:background="@drawable/white" 
	android:layout_width="fill_parent" android:layout_height="fill_parent">

	<TextView android:id="@+id/autotest_type" style="@style/NormalText"
		android:layout_alignParentLeft="true" android:layout_alignParentTop="true"
		android:textSize="17.0dip"
		android:text="@string/null_item" />

	<TextView android:id="@+id/item1" style="@style/NormalText"
		android:layout_below="@+id/autotest_type" android:maxLines="18"
		android:textSize="17.0dip"
		android:text="@string/null_item" />

	<TextView android:id="@+id/item_name" style="@style/NormalText"
		android:layout_below="@+id/item1" 
		android:textSize="17.0dip" 
		android:text="@string/null_item" />

	<TextView android:id="@+id/descript" style="@style/NormalText"
		android:layout_below="@+id/item_name" 
		android:textSize="17.0dip"
		android:text="@string/null_item" />

	<include layout="@layout/pass_fail_buttons"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:layout_alignParentBottom="true" />
</RelativeLayout>

<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout android:orientation="vertical"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<TextView android:textSize="20.0dip" android:layout_width="fill_parent"
		android:layout_marginTop="30dip" android:id="@+id/tv_calibration"
		android:layout_height="wrap_content" android:text="@string/proximity_calibration_info" />
	<Button android:textSize="20.0dip" android:id="@+id/m_calibration"
		android:layout_centerInParent="true" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="@string/test_calibration_title" />
	<include layout="@layout/pass_fail_buttons"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:layout_alignParentBottom="true" />
</RelativeLayout>